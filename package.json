{
  "name": "visualizer-on-tabs",
  "version": "0.0.0",
  "description": "",
  "type": "module",
  "module": "main/index.js",
  "sideEffects": false,
  "browserslist": [
    "defaults"
  ],
  "scripts": {
    "clean": "shx rm -rf out",
    "copy-static": "shx mkdir -p out && shx cp -r src/content/* out",
    "dev": "node bin/build.js --debug=1 --config=./dev.json && npx serve out/",
    "prettier": "prettier --check ./",
    "prettier-write": "prettier --write ./",
    "eslint": "eslint bin main src",
    "webpack": "node bin/build.js --debug=1",
    "watch": "npm run copy-static && npm run webpack -- --watch",
    "build": "npm run clean && npm run webpack",
    "test": "node --test && npm run eslint"
  },
  "dependencies": {
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "babel-loader": "^10.0.0",
    "bootstrap": "^5.3.3",
    "fs-extra": "^7.0.1",
    "iframe-bridge": "^1.1.0",
    "js-yaml": "^3.12.1",
    "lockr": "^0.8.5",
    "lodash": "^4.17.21",
    "minimist": "^1.2.6",
    "react": "^18.3.1",
    "react-bootstrap": "^3.0.0-beta.3",
    "react-dom": "^18.3.1",
    "react-visualizer": "^3.0.1",
    "superagent": "^4.1.0",
    "webpack": "^5.91.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^9.34.0",
    "eslint-config-zakodium": "^16.0.0",
    "prettier": "^3.6.2",
    "shx": "^0.3.2"
  },
  "volta": {
    "node": "20.12.2"
  }
}
